    "start = '2024-01-01'\n",
    "end = '2024-06-30'\n",
    "\n",
    "# Create a Ticker object\n",
    "ticker = yf.Ticker(symbol)\n",
    "\n",
    "# Download the data\n",
    "data = ticker.history(start=start, end=end)\n",
    "data"
   ]
